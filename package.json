{
  "name": "typescript-api",
  "version": "1.0.0",
  "description": "Complete API built with Node.js and Typescript",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "yarn build && node dist/src/index.js",
    "start:local": "ts-node-dev 'src/index.ts'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix",
    "test:functional": "jest --projects ./test --runInBand",
    "test:unit": "jest",
    "style:check": "prettier --check 'src/**/*.ts' 'test/**/*.ts'",
    "style:fix": "prettier --write 'src/**/*.ts' 'test/**/*.ts'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/robsoninocencio/node-typescript-api.git"
  },
  "keywords": [
    "node",
    "nodejs",
    "typescript",
    "api"
  ],
  "author": "Robson InocÃªncio",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/robsoninocencio/node-typescript-api/issues"
  },
  "homepage": "https://github.com/robsoninocencio/node-typescript-api#readme",
  "dependencies": {
    "@overnightjs/core": "^1.7.5",
    "axios": "^0.21.0",
    "body-parser": "^1.19.0",
    "config": "^3.3.3",
    "express": "^4.17.1",
    "module-alias": "^2.2.2",
    "mongoose": "^5.11.8"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/config": "^0.0.37",
    "@types/express": "^4.17.9",
    "@types/jest": "^26.0.19",
    "@types/module-alias": "^2.0.0",
    "@types/mongoose": "^5.10.3",
    "@types/nock": "^11.1.0",
    "@types/node": "^14.14.12",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "eslint": "^7.15.0",
    "jest": "^26.6.3",
    "nock": "^13.0.5",
    "prettier": "^2.2.1",
    "supertest": "^6.0.1",
    "ts-jest": "^26.4.4",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.2"
  }
}
